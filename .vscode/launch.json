{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Next.js",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "runtimeExecutable": "npx",
        "runtimeArgs": [
          "next", "dev"
        ],
        "outFiles": [
          "${workspaceFolder}/**/*.js"
        ]
      }
    ]
  }
  